class Switch1
{
	public static void main(String[] args) 
	{
		int i = 0;
		switch (i)
		{
			case 0 : 
			{
				System.out.println("In, Case 1...");
				break;
			}
			case 1 :
			{
				System.out.println("In, case 2...");
				break;
			}
			case 2 :
			{
				System.out.println("In, case 3...");
				break;
			}
			default :
			{
				System.out.println("In, Default...");
			}
		
		}
	}
}