class MethodExecutionManyNumberOfTime
{
	public static void main(String[] args)
	{
		System.out.println("I am main method");
		raju();
		raju();
		raju();
		raju();
		chutki();
		bheem();
	}
	public static int bheem(int a)
	{
		System.out.println("Bheem: Hey Raju i am there, Dont't worry");
		return a ;
	}
	public static void chutki()
	{
		System.out.println("Chutki: Get Lost!!");
	}
	public static void raju()
	{
		System.out.println("Raju: Hey chutki hii");
	}
}
