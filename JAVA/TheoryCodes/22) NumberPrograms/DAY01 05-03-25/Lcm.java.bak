class Lcm
{
	public static void main(String[] args)
	{
		System.out.println(lcm(15,8));
	}
	public static int lcm(int n1,int n2)
	{
		int max= n1>n2?n2:n2;
		int i=1;
		while(true)
		{
			if((max*i)%n1==0 && (max*i)%n2==0)
				return max*i;
			i++;
		}
	}
}
//we find the least common multiply means output is 77 ,the 77 will be divide by 7 as well as 11 ;thats why 7 and 11 lcm is 77